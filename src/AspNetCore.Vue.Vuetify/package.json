{
  "name": "aspnetcore_vue_vuetify",
  "description": "ASP.NET Core & VueJS & Typescript template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development NODE_ENV=development dotnet run",
    "build": "npm run build-vendor:prod && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-vendor:prod": "cross-env NODE_ENV=production webpack --config webpack.config.vendor.js --progress",
    "build-vendor:dev": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js --progress",
    "lint": "eslint -c ./.eslintrc.js ClientApp/**/*.js  ClientApp/**/*.vue  ClientApp/**/*.json webpack*.js",
    "install": "npm run build-vendor:dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.7.1",
    "animate.css": "3.7.0",
    "vuetify": "1.4.6",
    "jquery": "^3.3.1",
    "metismenu": "3.0.3",
    "nprogress": "0.2.0",
    "vue": "^2.6.1",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.1",
    "vue-template-compiler": "^2.6.1",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-brands-svg-icons": "^5.7.1",
    "@fortawesome/free-regular-svg-icons": "^5.7.1",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/vue-fontawesome": "0.1.5",
    "@types/jquery": "^3.3.29",
    "@types/webpack-env": "^1.13.7",
    "ajv": "^6.8.1",
    "aspnet-webpack": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "extract-css-chunks-webpack-plugin": "3.3.2",
    "event-source-polyfill": "^1.0.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.11.0",
    "node-sass-package-importer": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "popper.js": "1.14.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.2",
    "vue-property-decorator": "^7.3.0",
    "vue-style-loader": "4.1.2",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
